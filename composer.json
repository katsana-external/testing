{
    "name": "orchestra/testing",
    "description": "Orchestra Platform Testing Helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Testing\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2",
        "doctrine/dbal": "^2.5",
        "mockery/mockery": "^1.3.1",
        "orchestra/avatar": "^5.0",
        "orchestra/contracts": "^5.0",
        "orchestra/testbench": "^5.1",
        "orchestra/testbench-browser-kit": "^5.1"
    },
    "require-dev": {
        "orchestra/canvas": "^5.0",
        "orchestra/foundation": "^5.0",
        "orchestra/installer": "^5.0",
        "orchestra/platform": "5.x-dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
