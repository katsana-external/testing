{
    "name": "orchestra/testing",
    "description": "Orchestra Platform Testing Helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Testing\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "doctrine/dbal": "~2.5",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^0.9.4",
        "orchestra/avatar": "~3.3.0",
        "orchestra/contracts": "~3.3.0",
        "orchestra/testbench": "~3.3.0",
        "phpunit/phpunit": "~4.8|~5.0"
    },
    "require-dev": {
        "orchestra/foundation": "~3.3.0",
        "orchestra/installer": "~3.3.0"
    },
    "suggest": {
        "orchestra/installer": "Allow testing with installed Orchestra Platform (~3.3).",
        "orchestra/foundation": "Allow testing extension for Orchestra Platform (~3.3)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
