{
  "name": "orchestra/testing",
  "description": "Orchestra Platform Testing Helper",
  "license": "MIT",
  "authors": [
    {
      "name": "Mior Muhammad Zaki",
      "email": "crynobone@gmail.com",
      "homepage": "https://github.com/crynobone"
    }
  ],
  "autoload": {
    "psr-4": {
      "Orchestra\\Testing\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "doctrine/dbal": "^3.3",
    "orchestra/avatar": "7.x-dev",
    "orchestra/contracts": "^7.0",
    "orchestra/testbench": "^7.0",
    "orchestra/testbench-browser-kit": "^7.0"
  },
  "require-dev": {
    "orchestra/canvas": "^7.0",
    "orchestra/foundation": "7.x-dev",
    "orchestra/installer": "7.x-dev",
    "orchestra/platform": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/katsana-external/avatar"
    },
    {
      "type": "vcs",
      "url": "https://github.com/katsana-external/foundation"
    },
    {
      "type": "vcs",
      "url": "https://github.com/katsana-external/installer"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "7.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
