{
    "name": "orchestra/testing",
    "description": "Orchestra Platform Testing Helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Testing\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "orchestra/contracts": "~3.1.0",
        "orchestra/testbench": "~3.1.3"
    },
    "require-dev": {
        "orchestra/foundation": "~3.1.0"
    },
    "suggest": {
        "orchestra/foundation": "Allow testing extension for Orchestra Platform (~3.1)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    },
    "minimum-stability": "dev"
}
